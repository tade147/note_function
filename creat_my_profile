def build_profile(first, last, **info):
    """创建个人简历"""
    profile = {}
    profile['first_name'] = first
    profile['last_name'] = last
    for key, value in info.items():
        profile[key] = value
    return profile

my_profile = build_profile('wu', 'min', age='26', location='yichun', work='engneer')
print(my_profile)
